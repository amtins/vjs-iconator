{
  "log": "verbose",
  "dest": ".",
  "mode": {
    "symbol": {
      "sprite": "vjs-sprite-icons.svg",
      "dest": "vjs-sprite",
      "example": { "dest": "index.html" }
    }
  },
  "shape": {
    "id": { "generator": "vjs-icon-%s" },
    "dimension": {
      "maxWidth": 48,
      "maxHeight": 48
    }
  },
  "svg": {
    "xmlDeclaration": false,
    "doctypeDeclaration": false
  }
}
